{
  "name": "multi-channel-ai-agent",
  "version": "1.0.0",
  "description": "Scalable multi-channel AI agent service for WhatsApp, Instagram, and Web Chat",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "start": "node dist/app.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --runInBand",
    "test:unit": "jest --testPathPattern=tests/unit --runInBand",
    "test:integration": "jest --testPathPattern=tests/integration --runInBand --testTimeout=120000",
    "test:integration:load": "jest --testPathPattern=tests/integration/load-test --runInBand --testTimeout=120000",
    "test:integration:stress": "jest --testPathPattern=tests/integration/stress-test --runInBand --testTimeout=180000",
    "test:integration:latency": "jest --testPathPattern=tests/integration/latency-test --runInBand --testTimeout=60000",
    "test:integration:benchmark": "jest --testPathPattern=tests/integration/performance-benchmark --runInBand --testTimeout=180000",
    "test:verify": "node scripts/verify-test-setup.js",
    "stress:check": "node tests/stress/check-readiness.js",
    "stress:setup": "node tests/stress/setup-test-user.js",
    "stress:run": "node tests/stress/webchat-stress-test.js",
    "stress:test": "npm run stress:setup && npm run stress:run",
    "check-google-tokens": "node scripts/check-google-tokens.js",
    "test:ci": "jest --coverage --runInBand --ci --maxWorkers=2",
    "test:verbose": "jest --verbose --runInBand",
    "test:silent": "jest --silent --runInBand",
    "test:changed": "jest --onlyChanged --runInBand",
    "test:related": "jest --findRelatedTests",
    "pretest": "npm run lint",
    "migrate": "node scripts/migrate.js",
    "migrate:prod": "NODE_ENV=production node scripts/migrate.js",
    "seed": "node scripts/seed-test-data.js",
    "db:reset": "npm run migrate && npm run seed",
    "workers": "node scripts/start-workers.js",
    "worker:extraction": "node scripts/start-extraction-worker.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "quality-gate": "npm run lint && npm run test:coverage"
  },
  "keywords": [
    "ai",
    "chatbot",
    "whatsapp",
    "instagram",
    "multi-channel",
    "openai",
    "redis",
    "postgres"
  ],
  "author": "Multi-Channel AI Agent Team",
  "license": "MIT",
  "dependencies": {
    "@types/express-session": "^1.18.2",
    "axios": "^1.6.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.2",
    "googleapis": "^161.0.0",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "openai": "^4.20.1",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/pg": "^8.10.7",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.1.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "jest --bail --findRelatedTests --passWithNoTests"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test:coverage"
    }
  }
}
